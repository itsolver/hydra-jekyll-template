var options = {
        text: "Message 1",
        duration: 6e3,
        close: !0,
        callback: function () {
            this.remove(),
            Toastify.reposition()
        }
    },
    myToast = Toastify(options);
setTimeout(function () {
    myToast.showToast()
}, 1e3),
Toastify({text: "Message 2", duration: 5e3}).showToast();
